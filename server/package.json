{
  "name": "mail-tracker-server",
  "version": "1.0.0",
  "description": "TypeScript Express.js server for Mail Tracker",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "clean": "rimraf dist",
    "rebuild": "npm run clean && npm run build",
    "seed": "ts-node src/scripts/seedTestUser.ts",
    "seed-packages": "ts-node src/scripts/seedSamplePackages.ts",
    "delete-test-user": "ts-node src/scripts/deleteTestUser.ts",
    "delete-packages": "ts-node src/scripts/deleteAllPackages.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@types/xml2js": "^0.4.14",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "kill-port": "^2.0.1",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.4",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.0",
    "@types/nodemailer": "^6.4.9",
    "@types/supertest": "^6.0.3",
    "jest": "^30.1.3",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "mail",
    "tracker",
    "typescript",
    "express",
    "mongodb"
  ],
  "author": "Mail Tracker Team",
  "license": "MIT"
}
